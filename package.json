{
	"name": "identifier-session",
	"version": "1.0.0",
	"description": "API of session token information",
	"scripts": {
		"deploy": "AWS_PROFILE=privally SLS_DEBUG=* sls deploy",
		"docker": "docker-compose up --force-recreate",
