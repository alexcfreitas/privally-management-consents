		"eslint-plugin-standard": "^4.0.1",
		"prettier": "^2.0.5",
		"prettier-eslint": "^9.0.1",
		"serverless": "^1.73.1",
		"serverless-aws-documentation": "^1.1.0",
		"serverless-offline": "^6.4.0",
		"serverless-offline-lambda": "^1.0.6",
		"chai": "^4.2.0",
		"mocha": "^8.1.1",
		"serverless-dynamodb-local": "0.2.38"
	},
